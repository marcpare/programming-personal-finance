div
  if this.transactions
    table
      tr
        th Date
        th Description
        th Amount
        th Account
      for tr in this.transactions.data
        tr
          td #{h.date(tr.Date)}
          td
            | #{tr.Description}
            div.quiet #{tr['Original Description']}
          td #{h.dollars(tr.Amount)}
          td #{tr['Account Name']}